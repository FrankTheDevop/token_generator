    "loopback": "^3.0.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.4.0",
    "loopback-component-mq": "^2.2.5",
    "loopback-connector-mysql": "^5.4.2",
    "moment": "^2.24.0",
