    "mocha": "^6.2.0",
    "nsp": "^2.1.0",
    "nyc": "^14.1.1",
    "sinon": "^1.13.0",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0",
    "trim-right": "^1.0.1"
  },
  "repository": {
    "type": "",
    "url": ""
  },
