{
  "name": "lbseed",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "nps",
    "posttest": "npm run lint && nsp check",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --timeout=3000 --recursive test/*"
  },
  "dependencies": {
    "bluebird": "latest",
    "bunyan": "^1.8.12",
    "compression": "^1.0.3",
    "config": "^1.29.2",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "loopback-component-mq": "^2.2.5",
    "loopback-connector-mongodb": "^3.6.0",
    "loopback-connector-mysql": "^5.2.0",
    "loopback-connector-rest": "^2.1.0",
    "loopback-ds-timestamp-mixin": "^3.4.1",
    "nps": "^5.7.1",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "uuid": "^3.1.0",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-json": "^1.0.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.0.0",
    "nsp": "^2.1.0",
    "nyc": "^10.0.0",
    "sinon": "^1.13.0",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0",
    "trim-right": "^1.0.1"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "reporter": [
      "text-lcov",
      "text",
      "html"
    ],
    "temp-directory": "./alternative-tmp",
    "report-dir": "./coverage",
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  },
  "license": "UNLICENSED",
  "description": "My personal seed project for loopback to reduce setup times."
}
